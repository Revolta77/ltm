{
    "name": "revolta77/ltm",
    "description": "Enhanced Laravel Translation Manager",
    "keywords": [
        "laravel",
        "translations",
        "translator",
        "languages"
    ],
    "homepage": "https://github.com/Revolta77/ltm",
    "license": "MIT",
    "authors": [
        {
            "name": "Dušan Rybár",
            "email": "revolta77@gmail.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": ">=7.2.5",
        "illuminate/support": ">=7.0",
        "laravelcollective/html": "^6.1",
        "illuminate/translation": ">=7.0",
        "doctrine/dbal": "^2.10",
        "symfony/finder": "^5.0"
    },
    "require-dev": {
        "phpspec/phpspec": "~6.1",
        "fzaninotto/faker": "~1.9",
        "mockery/mockery": "^1.3",
        "phpunit/phpunit": "^8.5"
    },
    "autoload": {
        "classmap": [
            "src/Classes"
        ],
        "files": [
            "src/Support/helpers.php"
        ],
        "psr-4": {
            "Revolta77\\TranslationManager\\": "src/"
        }
    },
    "autoload-dev": {
        "classmap": [
            "tests/"
        ],
        "psr-4": {
        }
    },
    "scripts": {
        "post-update-cmd": [
        ]
    },
    "minimum-stability": "stable"
}
